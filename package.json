{
  "name": "create-ao-app",
  "version": "1.0.4",
  "description": "CLI tool to create AO-powered applications",
  "type": "module",
  "bin": {
    "create-ao-app": "./dist/index.js",
    "cao": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "prepublishOnly": "npm run build",
    "prepare": "rimraf dist && npm run build",
    "test": "jest",
    "test:cli": "npm run build && node dist/index.js",
    "link:global": "npm run prepare && cross-env-shell \"[[ \\\"$OS\\\" != 'Windows_NT' ]] && chmod +x dist/index.js || exit 0\" && npm link",
    "unlink:global": "npm unlink -g create-ao-app",
    "relink:global": "npm run unlink:global && npm run link:global"
  },
  "keywords": [
    "ao",
    "arweave",
    "cli",
    "nextjs",
    "nuxtjs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^12.1.0",
    "js-yaml": "^4.1.0",
    "open": "^10.0.0",
    "openai": "^4.73.0",
    "ora": "^8.1.1",
    "which": "^5.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.9.3",
    "@types/which": "^3.0.4",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
