{
  "name": "create-ao-app",
  "version": "1.0.3",
  "description": "CLI tool to create AO-powered applications",
  "type": "module",
  "bin": {
    "create-ao-app": "./dist/index.js",
    "cao": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "prepublishOnly": "pnpm build",
    "prepare": "rm -rf dist && pnpm build",
    "test": "jest",
    "test:cli": "pnpm run build && node dist/index.js",
    "link:global": "pnpm run prepare && chmod +x dist/index.js && npm link",
    "unlink:global": "npm unlink -g create-ao-app",
    "relink:global": "pnpm run unlink:global && pnpm run link:global"
  },
  "keywords": [
    "ao",
    "arweave",
    "cli",
    "nextjs",
    "nuxtjs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^12.1.0",
    "js-yaml": "^4.1.0",
    "ora": "^8.1.1",
    "which": "^5.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.9.0",
    "@types/which": "^3.0.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}